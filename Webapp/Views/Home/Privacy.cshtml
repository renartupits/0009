@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<h3>Claims</h3>
<table>
    <tr>
        <th>Type</th>
        <th>value</th>
        <th>value type</th>
    </tr>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
            <td>@claim.ValueType</td>
        </tr>
    }
</table>

